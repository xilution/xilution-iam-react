{
  "name": "@xilution/xilution-iam-react",
  "version": "1.0.1",
  "description": "Xilution Identity and Access Management React Components",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .cache coverage dist",
    "clean:all": "yarn clean && rm -rf node_modules",
    "lint": "yarn lint:tslint",
    "lint:tslint": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\" \"./test/**/*.ts\" \"./test/**/*.tsx\"",
    "test": "yarn test:unit",
    "test:unit": "jest --testPathPattern ./test/unit/",
    "verify": "yarn lint && yarn test",
    "watch": "npx nodemon -e ts,tsx --watch src --exec \"yarn build\"",
    "release": "yarn verify && yarn build && npx semantic-release --ci false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xilution/xilution-iam-react"
  },
  "bugs": {
    "url": "https://github.com/xilution/xilution-iam-react/issues"
  },
  "homepage": "https://github.com/xilution/xilution-iam-react",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/uuid": "^3.4.5",
    "axios": "^0.19.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.5.2",
    "utility-types": "^3.7.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.4.3",
    "@semantic-release/npm": "^5.1.15",
    "@types/chance": "^1.0.6",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/jest-image-snapshot": "^2.8.0",
    "@types/node": "^12.7.2",
    "@types/node-sass": "^4.11.0",
    "babel-jest": "^24.8.0",
    "chance": "^1.0.18",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.4.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-image-snapshot": "^2.5.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "regenerator-runtime": "^0.13.3",
    "semantic-release": "^15.13.24",
    "stylelint": "^10.1.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.4",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.19.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.4",
    "typescript-tslint-plugin": "^0.5.3"
  },
  "engines": {
    "node": ">=8.10.0",
    "yarn": ">=1.3.2",
    "npm": ">=5.6.0"
  },
